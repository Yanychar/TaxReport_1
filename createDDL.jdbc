CREATE TABLE ORGANISATION (id BIGINT NOT NULL, ADDRESS VARCHAR(255), CODE VARCHAR(255), CONSISTENCYVERSION BIGINT NOT NULL, deleted BOOLEAN NOT NULL, EMAIL VARCHAR(255), INFO VARCHAR(255), NAME VARCHAR(255), PHONE VARCHAR(255), STRVALUE VARCHAR(255), TUNNUS VARCHAR(255), PRIMARY KEY (id))
CREATE TABLE TMSUSER (id BIGINT NOT NULL, CODE VARCHAR(255), CONSISTENCYVERSION BIGINT NOT NULL, deleted BOOLEAN NOT NULL, FIRSTNAME VARCHAR(255), LASTNAME VARCHAR(255), MIDNAME VARCHAR(255), ORGANISATION_id BIGINT, PRIMARY KEY (id))
CREATE TABLE TAXREPORT (id BIGINT NOT NULL, CODE VARCHAR(255), CONSISTENCYVERSION BIGINT NOT NULL, DATE BYTEA, deleted BOOLEAN NOT NULL, LASTMODIDATE BYTEA, SITES BYTEA, STATUS INTEGER, STRVALUE VARCHAR(255), TYPE INTEGER, ORGANISATION_id BIGINT, PRIMARY KEY (id))
ALTER TABLE TMSUSER ADD CONSTRAINT FK_TMSUSER_ORGANISATION_id FOREIGN KEY (ORGANISATION_id) REFERENCES ORGANISATION (id)
ALTER TABLE TAXREPORT ADD CONSTRAINT FK_TAXREPORT_ORGANISATION_id FOREIGN KEY (ORGANISATION_id) REFERENCES ORGANISATION (id)
